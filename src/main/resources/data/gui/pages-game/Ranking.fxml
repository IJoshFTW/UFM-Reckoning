<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane styleClass="pane" stylesheets="@Competition.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.joshuaslik.UFMReckoning.gui.game.RankingController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="rankingtable" opacity="0.9" prefHeight="532.0" prefWidth="776.0">
              <columns>
                <TableColumn fx:id="nr" editable="false" prefWidth="63.0" text="Rank" />
                <TableColumn fx:id="team" editable="false" prefWidth="276.0" text="Team" />
                  <TableColumn fx:id="totalWins" editable="false" prefWidth="54.0" text="Wins" />
                  <TableColumn fx:id="totalLosses" editable="false" prefWidth="51.0" text="Losses" />
                  <TableColumn fx:id="totalDraws" editable="false" prefWidth="81.0" text="Draws" />
                  <TableColumn fx:id="goals" editable="false" prefWidth="70.0" text="Goals" />
                  <TableColumn fx:id="goalsagainst" editable="false" prefWidth="102.0" text="GoalsAgainst" />
                  <TableColumn fx:id="totalPoints" editable="false" prefWidth="76.0" text="Points" />
              </columns>
               <HBox.margin>
                  <Insets bottom="50.0" left="25.0" right="25.0" top="50.0" />
               </HBox.margin>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
