<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane styleClass="pane" stylesheets="@NewGame.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.joshuaslik.UFMReckoning.gui.NewGameController">
   <children>
      <TableView fx:id="teamtable" opacity="0.63" prefHeight="396.0" prefWidth="562.0" AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="teamColumn" text="Team" />
          <TableColumn fx:id="coachColumn" text="Coach" />
            <TableColumn fx:id="averageattack" minWidth="0.0" text="Average Attack" />
            <TableColumn fx:id="averagedefence" text="Average Defence" />
            <TableColumn fx:id="averagestamina" minWidth="0.0" text="Average Stamina" />
            <TableColumn minWidth="0.0" text="Total Players" />
            <TableColumn minWidth="0.0" text="Team value" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleChooseTeam" text="Choose this team">
            </Button>
         </children></HBox>
   </children>
</AnchorPane>
